<!--pages/searchresult/searchresult.wxml-->

<!-- 搜索框 -->
<view class="search-wrap">
  <wxc-search class="search" bg-color="#f6f6f6" color="#000" icon-color="#cacff1" ph-color="#cacff1" placeholder="输入内容" button="搜索" btn-color="#2CB42F" bind:confirm="toSearch"></wxc-search>
</view>

<!-- 活动事件 -->
<view class='active' wx:for="{{active}}" bindtap='toActive'>
  <view class='act-title'>{{item.title}}</view>
  <view class='act-content'>{{item.content}}</view>
</view>